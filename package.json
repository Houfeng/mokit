{
  "name": "mokit",
  "version": "0.0.1",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "start": "nokit stop && nokit start --public ./ --port 8002",
    "clear": "rm -rf ./dist/** && mkdir -p ./.tmp && mkdir -p ./dist",
    "build": "npm run clear && node ./bin/info.js && browserify ./lib/index.js | uglifyjs -m  > ./dist/bundle.js",
    "watch": "npm run clear && node ./bin/info.js && watchify ./lib/index.js -o 'exorcist ./dist/bundle.js.map > ./dist/bundle.js' -d"
  },
  "author": "",
  "license": "Apache License",
  "devDependencies": {
    "browserify": "^13.1.0",
    "exorcist": "^0.4.0",
    "nokitjs": "^1.26.1",
    "selenium-webdriver": "^3.0.0",
    "uglifyify": "^3.0.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "cify": "^1.0.2",
    "ntils": "^1.1.3"
  }
}
