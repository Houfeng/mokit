{
  "name": "mokit",
  "version": "0.0.1",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "start": "nokit stop && nokit start --public ./ --port 8002",
    "clear": "rm -rf ./dist/** && mkdir -p ./.tmp && mkdir -p ./dist",
    "build": "npm run clear && node ./bin/info.js && browserify ./lib/index.js | uglifyjs -m  > ./dist/$npm_package_name.js",
    "watch": "npm run clear && node ./bin/info.js && watchify ./lib/index.js -o 'exorcist ./dist/$npm_package_name.js.map > ./dist/$npm_package_name.js' -d",
    "test": "echo ..."
  },
  "author": "",
  "license": "Apache License",
  "devDependencies": {
    "browserify": "^13.1.1",
    "cify": "^1.0.2",
    "exorcist": "^0.4.0",
    "nokitjs": "^1.26.1",
    "ntils": "^1.2.0",
    "selenium-webdriver": "^3.0.1",
    "uglifyify": "^3.0.4",
    "watchify": "^3.7.0"
  }
}
